{
  "name": "fczlicin-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "APP_ENV=development npx expo run:android",
    "ios": "APP_ENV=development npx expo run:ios",
    "web": "expo start --web",
    "run:android": "APP_ENV=development npx expo run:android",
    "run:ios": "APP_ENV=development npx expo run:ios",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "build:apk": "APP_ENV=production NODE_ENV=production npx expo prebuild --platform android && cd android && APP_ENV=production NODE_ENV=production ./gradlew assembleRelease",
    "install:apk": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "build:aab": "APP_ENV=production NODE_ENV=production npx expo prebuild --platform android && cd android && APP_ENV=production NODE_ENV=production ./gradlew bundleRelease",
    "build:ios:prod": "APP_ENV=production NODE_ENV=production npx expo prebuild --platform ios",
    "build:aab:clean": "APP_ENV=production NODE_ENV=production npx expo prebuild --clean --platform android && cd android && APP_ENV=production NODE_ENV=production ./gradlew bundleRelease",
    "build:ios:prod:clean": "APP_ENV=production NODE_ENV=production npx expo prebuild --clean --platform ios",
    "firebase:dev": "node scripts/copy-firebase-config.js dev",
    "firebase:prod": "node scripts/copy-firebase-config.js prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
    "@expo-google-fonts/rajdhani": "^0.2.3",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/analytics": "^20.0.0",
    "@react-native-firebase/app": "^20.0.0",
    "@react-native-firebase/crashlytics": "^20.0.0",
    "@react-native-firebase/messaging": "^20.0.0",
    "@react-native-firebase/remote-config": "^20.0.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "dayjs": "^1.11.19",
    "expo": "^54.0.32",
    "expo-asset": "~12.0.12",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-linking": "~8.0.11",
    "expo-navigation-bar": "~5.0.10",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.22",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-expo": "~54.0.2",
    "detox": "^20.14.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.16",
    "prettier": "^3.3.3",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
